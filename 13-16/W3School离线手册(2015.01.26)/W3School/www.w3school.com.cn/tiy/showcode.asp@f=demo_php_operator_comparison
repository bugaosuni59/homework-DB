$x=100; 
$y=&quot;100&quot;;

var_dump($x == $y); // 因为值相等，返回 true
echo &quot;&lt;br&gt;&quot;;
var_dump($x === $y); // 因为类型不相等，返回 false
echo &quot;&lt;br&gt;&quot;;
var_dump($x != $y); // 因为值相等，返回 false
echo &quot;&lt;br&gt;&quot;;
var_dump($x !== $y); // 因为值不相等，返回 true
echo &quot;&lt;br&gt;&quot;;

$a=50;
$b=90;

var_dump($a &gt; $b);
echo &quot;&lt;br&gt;&quot;;
var_dump($a &lt;  $b);
