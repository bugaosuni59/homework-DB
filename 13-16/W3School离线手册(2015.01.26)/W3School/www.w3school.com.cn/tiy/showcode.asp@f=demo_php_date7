$startdate=strtotime(&quot;Saturday&quot;);
$enddate=strtotime(&quot;+6 weeks&quot;,$startdate);

while ($startdate &lt;  $enddate) {
   echo date(&quot;M d&quot;, $startdate),&quot;&lt;br&gt;&quot;;
   $startdate = strtotime(&quot;+1 week&quot;, $startdate);
}
