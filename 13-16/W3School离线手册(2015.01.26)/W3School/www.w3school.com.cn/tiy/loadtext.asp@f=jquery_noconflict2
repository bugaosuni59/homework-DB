<script src="../jquery/jquery-1.11.1.min.js"></script>
<script>
var jq=$.noConflict();
jq(document).ready(function(){
  jq("button").click(function(){
    jq("p").text("jQuery 仍在运行！");
  });
});
</script>
</head>

<body>
